{
  "expo": {
    "name": "Husky",
    "description": "This project is really great.",
    "slug": "Husky",
    "privacy": "public",
    "sdkVersion": "25.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lightningvii.husky"
    },
    "android": {
      "package": "com.lightningvii.husky"
    },
    "assetBundlePatterns": ["assets/**"]
  }
}
